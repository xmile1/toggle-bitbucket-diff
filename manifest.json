{
  "name": "Bitbucket Diff File Toggle",
  "description": "Toggle Bitbucket diff files just like you do in github and gitlab",
  "version": "0.0.2",
  "short_name": "BB Diff Toggle",
  "author": "Uyiosa Enabulele",
  "icons": {
    "16": "diff_toggle.png",
    "48": "diff_toggle.png",
    "128": "diff_toggle.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
       "https://bitbucket.org/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}